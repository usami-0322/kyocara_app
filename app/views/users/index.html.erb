<div class="main users-index">
  <div class="container">
    <h1>ユーザー一覧</h1>
    <% @users.each do |user| %>
    <div class="user-index-item">
      <div class="user-left">
        <img src="<%= "/#{user.image_name}" %>">
      </div>
      <div class="user-right">
      <%= link_to(user.name, "/users/#{user.id}") %>
    </div>
    <% end %>
    </div>
  </div>
</div>
